citHeader("To cite AugspurgerIndex in publications use:")

citEntry(
  entry    = "Misc",
  title    = "AugspurgerIndex",
  author   = as.person("Reed Clark Benkendorf"),
  year     = 2023,
  url      = "https://github.com/sagesteppe/AugspurgerIndex",
  textVersion = textVersion = paste0(author, "(", year, "). ", title, url)
)
